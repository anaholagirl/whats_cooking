<div class="col-md-4 login_box">
  <div class="login_entry">
    <h1><p>Welcome Back!</p>
      <p>Please Sign In</p></h1>
    <%= form_tag sessions_path do %>
      <%= label_tag :email %>
      <p><%= text_field_tag :email, params[:email] %></p>
      <%= label_tag :password %>
      <p><%= password_field_tag :password %></p>
      <p><%= submit_tag 'Log In', class: 'btn btn-primary btn-lg' %></p>
    <% end %>
  </div>
</div>
