<!DOCTYPE html>
<html>
<head>
 <title><%= yield(:title) %></title>
   <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="wrapper">

    <div id="header">
      <%= render 'layouts/header' %>
    </div>

    <div id="content">



      <p class="bg-success", id="notice"><%= flash[:notice] %></p>
      <p class="bg-danger", id="alert"><%= flash[:alert] %></p>

      <%= yield %>

    </div>

    <div id="footer">

      <%= render 'layouts/footer' %>

    </div>
  </div>
</body>
</html>
